<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>RegistrationServlet</servlet-name>
  	<display-name>RegistrationServlet</display-name>
  	<description></description>
  	<servlet-class>com.amit.servlet.RegistrationServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>BookServlet</servlet-name>
  	<display-name>BookServlet</display-name>
  	<description></description>
  	<servlet-class>com.amit.servlet.BookServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>BookProcess</servlet-name>
  	<display-name>BookProcess</display-name>
  	<description></description>
  	<servlet-class>com.amit.servlet.BookProcess</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>RegistrationServlet</servlet-name>
  	<url-pattern>/RegistrationServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>BookServlet</servlet-name>
  	<url-pattern>/BookServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>BookProcess</servlet-name>
  	<url-pattern>/BookProcess</url-pattern>
  </servlet-mapping>
</web-app>
